<h1>Listing nuevo_usuarios</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Nickname</th>
    <th>Peso</th>
    <th>Edad</th>
    <th>Altura</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @nuevo_usuarios.each do |nuevo_usuario| %>
  <tr>
    <td><%= nuevo_usuario.nombre %></td>
    <td><%= nuevo_usuario.nickname %></td>
    <td><%= nuevo_usuario.peso %></td>
    <td><%= nuevo_usuario.edad %></td>
    <td><%= nuevo_usuario.altura %></td>
    <td><%= link_to 'Show', nuevo_usuario %></td>
    <td><%= link_to 'Edit', edit_nuevo_usuario_path(nuevo_usuario) %></td>
    <td><%= link_to 'Destroy', nuevo_usuario, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

