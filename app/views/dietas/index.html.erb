<h1>Listing dietas</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Dia</th>
    <th>Descripcion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dietas.each do |dieta| %>
  <tr>
    <td><%= dieta.nombre %></td>
    <td><%= dieta.dia %></td>
    <td><%= dieta.descripcion %></td>
    <td><%= link_to 'Show', dieta %></td>
    <td><%= link_to 'Edit', edit_dieta_path(dieta) %></td>
    <td><%= link_to 'Destroy', dieta, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dieta', new_dieta_path %>
